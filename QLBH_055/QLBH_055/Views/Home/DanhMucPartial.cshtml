
@using QLBH_055.Models;
@model List<DANHMUC>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link href="~/Content/CssClient/CssDanhMuc.css" rel="stylesheet" />
<ul class="choose-category">
    @foreach (var item in Model)
    {
        <li class="category">
            <a href="~/Trang-Chu?MADM=@item.MADM" class="category-item">

                <label for="warning" class="btn btn-warning">@item.TENDM<input id="warning" type="checkbox" name="price"  class="badgebox"></label>

                @*<label for="success" class="btn btn-success"> @item.TENDM<input id="success" type="checkbox" name="price" class="badgebox"><span class="badge">&check;</span></label>*@
            </a>

        </li>
    }
</ul>




