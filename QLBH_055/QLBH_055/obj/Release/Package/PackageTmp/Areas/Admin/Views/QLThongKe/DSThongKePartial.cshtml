
<table class="table table-striped">
    <thead >
        <tr>
            <th>Mã hóa đơn</th>
            <th>Mã khách hàng</th>
            <th>Tên khách hàng</th>
            <th>Điện thoại</th>
            <th>Địa chỉ</th>
            <th>Đơn giá</th>
            <th>Ngày đặt</th>
            <th>Ngày giao</th>
            <th>Hình thức thanh toán</th>
            <th>Hình thức giao hàng</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.DS)
        {
            var ngaydat = "";
            var ngaygiao = "";
            ngaydat = String.Format("{0:d}", @item.NGAYDAT);
            if (item.NGAYGIAO == null)
            {
                ngaygiao = @item.NGAYGIAO;
            }
            else
            {
                ngaygiao = String.Format("{0:d}", @item.NGAYGIAO);
            }
            <tr>
                <td>@item.MAHD</td>
                <td>@item.MAKH</td>
                <td>@item.TENKH</td>
                <td>@item.DIENTHOAI</td>
                <td>@item.DIACHI</td>
                <td>@item.DONGIA</td>
                <td>@ngaydat</td>

                <td>
                    @ngaygiao

                </td>


                <td>@item.HTTHANHTOAN</td>
                <td>@item.HTGIAOHANG</td>
            </tr>
        }
    </tbody>
</table>